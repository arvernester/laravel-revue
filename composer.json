{
  "name": "yugo/revue",
  "description": "Unoffical getrevue.io package for Laravel framework",
  "license": "MIT",
  "keywords": [
    "revue"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Yugo",
      "email": "dedy.yugo.purwanto@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^9.4"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Yugo\\Revue\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yugo\\Revue\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yugo\\Revue\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
